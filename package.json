{
  "name": "astro-wordpress-demo",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/vercel": "^3.3.0",
    "astro": "^2.4.1"
  },
  "devDependencies": {
    "prettier": "^2.8.4",
    "prettier-plugin-astro": "^0.8.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      },
      {
        "files": "*.json",
        "options": {
          "parser": "json-stringify"
        }
      }
    ]
  }
}
